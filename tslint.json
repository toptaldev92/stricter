{
    "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "**/*.json"
        ]
    },
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
        "ter-indent": false,
        "align": false,
        "strict-boolean-expressions": false,
        "import-name": false,
        "prettier": true
    }
}
